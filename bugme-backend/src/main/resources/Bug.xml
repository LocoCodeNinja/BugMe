<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="team13.bugme.entity.Bug">
        <table name="bug" schema="dbo" catalog="Team13_BugMe"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="severity">
                <column name="severity" length="10"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
