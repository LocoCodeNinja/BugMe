<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="team13.bugme.entity.Account">
        <table name="account" schema="dbo" catalog="Team13_BugMe"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username" length="50"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="role">
                <column name="role" length="20"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
